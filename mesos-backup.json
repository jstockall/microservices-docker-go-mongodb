{
    "id": "cinema/backup",
    "container": {
      "type": "DOCKER",
      "docker": {
          "image": "stockallj/cinema-backup:201703071608",
          "network": "BRIDGE",
          "portMappings": [
            {
              "protocol": "tcp",
              "containerPort": 8080,
              "labels": {
                "VIP_0": "/backup:80"
              },
              "name": "backup-vip"
            }
          ]
      }
    },
    "acceptedResourceRoles": ["slave_public"],
    "instances": 1,
    "cpus": 0.25,
    "mem": 512,
    "disk": 0,
    "env": { "DATABASE_HOST":"db.marathon.l4lb.thisdcos.directory" }
}
